{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "semi": ["error", "never"],
      "quote-props": ["error", "consistent"],
      "new-cap": ["error", {
        "capIsNewExceptions": ["Map", "List"]
        }],
      "no-unused-vars": ["error", {
        "argsIgnorePattern": "_"
        }]
    }
}
